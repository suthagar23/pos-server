 *         content:
 *             application/x-www-form-urlencoded:
 *             schema:
 *                 required:
 *                      gitUrl
 *                 type: object
 *                 properties:
 *                     appName:
 *                         type: string
 *                         description: the id (or email address) inserted by the user in the login form
 *                     gitUrl:
 *                         type: string
 *                         format: password


  *     parameters:
 *       - name: body
 *       - in: body
 *         name: userName
 *         schema:
 *             type: object
 *         required: true
 *         description: Insert your userName
 *       - in: x-www-form-urlencoded
 *         name: password
 *         schema:
 *             type: string
 *             format: password
 *         required: true
 *         description: Insert your password


 *         schema:
 *             type: object
 *             properties:
 *                  type: object
 *                  properties:     
 *                      msgId: 
 *                          type: string
 *                          description: Insert your userName